<h1>List of Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Result</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% (1..100).each do |number| %>
      <tr>
        <td><%= number %></td>
        <td><%= "RESULT" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
